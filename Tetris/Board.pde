public class Board {
  int score;
  int level;
  int tickSpeed;
  int linesCleared;
  int[][] board;

  int getScore() {
    return score;
  }
  int getLevel() {
    return level;
  }
  int getTickSpeed() {
    return tickSpeed;
  }
  int getLinesCleared() {
    return linesCleared;
  }
  int[][] getBoard() {
    return board;
  }
}
